{
  "name": "admin-panel-playwright-typescript-test",
  "version": "4.0",
  "description": "Playwright e2e Framework with Typescript",
  "scripts": {
    "start": "npx patch-package&&vite --port=1234 --open",
    "start:staging": "npx patch-package&&cross-env NODE_ENV=staging vite  --port=1234 --open",
    "start:demo": "npx patch-package&&cross-env NODE_ENV=demo vite --port=1234 --open",
    "start:local": "npx patch-package&&cross-env NODE_ENV=local vite --port=1234 --open",
    "start:ngrok": "npx patch-package&&cross-env NODE_ENV=ngrok vite --port=1234 --open",
    "preview": "vite preview --port=1234 --open",
    "build": "npx patch-package&&vite build --emptyOutDir",
    "build:staging": "npx patch-package&&npx tsc --noEmit&&cross-env NODE_ENV=staging vite build --emptyOutDir",
    "version": "npm version patch",
    "parcel_build": "npx patch-package&&parcel build ./src/index.html --no-source-maps",
    "parcel_build:staging": "npx patch-package&&npx tsc --noEmit&&cross-env NODE_ENV=staging parcel build ./src/index.html --no-source-maps",
    "parcel_clean": "rimraf ./dist&&rimraf ./.parcel-cache&&rimraf ./node_modules",
    "parcel_start": "npx patch-package&&parcel serve src/index.html --open --https --no-source-maps",
    "parcel_start:staging": "npx patch-package&&cross-env NODE_ENV=staging parcel serve src/index.html --open --no-source-maps --https",
    "parcel_start:local": "npx patch-package&&cross-env NODE_ENV=local parcel serve src/index.html --open --no-source-maps --https",
    "start-tests": "npx patch-package&&parcel serve src/index.html --no-source-maps --port 2345",
    "trans": "i18next&&node scripts/translations.js",
    "testgen": "npx patch-package&&npx playwright codegen --load-storage=playwright-storageState.json --save-storage=playwright-storageState.json --ignore-https-errors https://localhost:1234",
    "test:debug": "npx patch-package&&cross-env PWDEBUG=1 yarn test",
    "ts": "npx tsc --noEmit > ts_errors.txt",
    "test": "npx playwright test",
    "test:chromium": "npx playwright test --project=chromium",
    "test:chrome": "npx playwright test --project=chrome",
    "test:firefox": "npx playwright test --project=firefox",
    "test:safari": "npx playwright test --project=webkit",
    "test:mobile": "npx playwright test --project=mobile",
    "lighthouse": "node e2e/test/performance/Lighthouse.js",
    "allure-reporter": "npm run clean && playwright test --config=playwright.allure.config.js",
    "monocart-reporter": "npm run clean && playwright test --config=playwright.monocart.config.js"
  },
  "keywords": [
    "node",
    "javascript",
    "allure-report",
    "rimraf",
    "lambdatest",
    "allure-reporting",
    "playwright-tests",
    "lambdatest-playwright"
  ],
  "author": "Denis Gershengoren",
  "dependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@types/crypto-js": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "exceljs": "^4.4.0",
    "lighthouse": "^12.5.1",
    "ortoni-report": "^2.0.3",
    "pdfjs-dist-es5": "^2.13.216",
    "pg": "^8.13.0",
    "winston": "^3.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/denis698/nexudus-playwright-e2e.git"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^22.14.0",
    "allure-commandline": "^2.20.1",
    "allure-playwright": "^3.1.0",
    "cross-env": "^7.0.3",
    "monocart-reporter": "^2.9.23",
    "rimraf": "^4.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
